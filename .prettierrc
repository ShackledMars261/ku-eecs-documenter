{
  "tabWidth": 2,
  "useTabs": false,
  "experimentalTernaries": true,
  "printWidth": 80,
  "bracketSameLine": true,
  "requirePragma": false,
  "insertPragma": true,
  "checkIgnorePragma": true,
  "endOfLine": "lf",
  "embeddedLanguageFormatting": "auto",
  "singleAttributePerLine": false,
  "plugins": [
    "prettier-plugin-organize-imports",
    "prettier-plugin-tailwindcss"
  ]
}
